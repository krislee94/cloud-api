{
  "name": "cloud-api",
  "version": "1.0.0",
  "description": "网易云音乐api",
  "egg": {
    "typescript": true
  },
  "dependencies": {
    "NeteaseCloudMusicApi": "3.33.1",
    "egg": "^2.15.1",
    "egg-alinode": "^2.0.1",
    "egg-onerror": "^2.1.0",
    "egg-scripts": "^2.13.0",
    "egg-validate": "^2.0.2",
    "is-type-of": "^1.2.1",
    "number-precision": "^1.5.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.12.14",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-ts-helper": "^1.25.8",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "set NODE_ENV=production && egg-scripts start --port=7001 --daemon --title=egg-server-cloud-api",
    "stop": "egg-scripts stop --title=egg-server-cloud-api",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
